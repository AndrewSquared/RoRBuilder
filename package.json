{
  "name": "ror-career-builder",
  "version": "1.0.0",
  "description": "Web app for users of computer game Return of Reckoning to build and save career specs",
  "main": "",
  "engines": {
    "node": "8.5.0",
    "npm": "5.5.1"
  },
  "scripts": {
    "lint": "eslint --fix src/js",
    "prod": "webpack --config webpack.prod.js",
    "dev": "webpack-dev-server --config webpack.dev.js --progress --colors",
    "postinstall": "./node_modules/.bin/webpack --config webpack.prod.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Natherul/RoRBuilder.git"
  },
  "keywords": [
    "webpack",
    "React",
    "es6",
    "babel",
    "warhammer",
    "Return of Reckoning",
    "Games Workshop",
    "CSS Modules",
    "Redux",
    "React Router"
  ],
  "author": "Matt Richards",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.2.3",
    "autoprefixer": "7.2.1",
    "autotrack": "0.6.5",
    "axios": "0.17.1",
    "babel-loader": "^8.0.4",
    "classnames": "2.2.5",
    "css-loader": "^2.1.0",
    "cssnano": "3.10.0",
    "express": "4.16.2",
    "handlebars": "4.0.11",
    "handlebars-loader": "^1.7.1",
    "html-webpack-plugin": "^3.2.0",
    "lodash": "4.17.4",
    "postcss-calc": "5.3.0",
    "postcss-import": "11.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-modules-values": "1.3.0",
    "postcss-nested": "2.1.2",
    "query-string": "^5.1.0",
    "react": "16.1.1",
    "react-dom": "16.1.1",
    "react-hot-loader": "^4.6.3",
    "react-redux": "5.0.6",
    "react-router-dom": "4.2.2",
    "redux": "3.7.2",
    "redux-devtools-extension": "2.13.2",
    "redux-promise": "0.5.3",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.28.3",
    "webpack-dev-server": "^3.1.14"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.0.0",
    "eslint": "^5.11.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.1",
    "mini-css-extract-plugin": "^0.5.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prettier": "^1.15.3",
    "webpack-cli": "^3.1.2"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "eslintConfig": {
    "extends": [
      "airbnb",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ]
    },
    "env": {
      "browser": true,
      "node": true
    }
  },
  "eslintIgnore": [
    "public"
  ]
}
