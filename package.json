{
  "name": "ror-career-builder",
  "version": "1.0.0",
  "description": "Web app for users of computer game Return of Reckoning to build and save career specs",
  "main": "",
  "engines": {
    "node": "8.5.0",
    "npm": "5.5.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "postinstall": "react-scripts build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Natherul/RoRBuilder.git"
  },
  "keywords": [
    "webpack",
    "React",
    "es6",
    "babel",
    "warhammer",
    "Return of Reckoning",
    "Games Workshop",
    "CSS Modules",
    "Redux",
    "React Router"
  ],
  "author": "Matt Richards",
  "license": "MIT",
  "dependencies": {
    "autotrack": "0.6.5",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "lodash": "^4.17.11",
    "query-string": "^6.2.0",
    "react": "^16.8.1",
    "react-app-polyfill": "^0.2.0",
    "react-dom": "^16.8.1",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.3",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-promise": "^0.6.0",
    "serve": "^10.1.2"
  },
  "devDependencies": {
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.1",
    "prettier": "^1.16.4"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "eslintConfig": {
    "extends": [
      "airbnb",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ]
    },
    "env": {
      "browser": true,
      "node": true
    },
    "parser": "babel-eslint"
  },
  "eslintIgnore": [
    "public"
  ],
  "browserslist": [
    ">0.5%",
    "not dead",
    "not ie < 11",
    "not op_mini all"
  ]
